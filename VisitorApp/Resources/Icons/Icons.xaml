            <Path.Data>
                <PathGeometry Figures="M21 12a9 9 0 1 1-6.219-8.56" FillRule="NonZero" />
            </Path.Data>
        </Path>
    </Canvas>

    <Canvas x:Key="ArrowLeft" Width="24" Height="24">
        <Path StrokeThickness="2" Stroke="{Binding Foreground, RelativeSource={RelativeSource AncestorType=ContentControl}}" StrokeLineJoin="Round" StrokeStartLineCap="Round" StrokeEndLineCap="Round">
            <Path.Data>
                <PathGeometry Figures="m12 19-7-7 7-7" FillRule="NonZero" />
